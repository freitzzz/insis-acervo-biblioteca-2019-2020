      <bpmndi:BPMNShape bpmnElement="decidirAquisicaoEnviarNotificacaoProponenteCallActivity" id="BPMNShape_decidirAquisicaoEnviarNotificacaoProponenteCallActivity">
        <omgdc:Bounds height="74.0" width="141.0" x="630.0" y="139.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge bpmnElement="flow1" id="BPMNEdge_flow1">
