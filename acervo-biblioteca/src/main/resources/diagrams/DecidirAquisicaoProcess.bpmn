      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="decidirAquisicaoEnviarNotificacaoProponenteAceiteCallActivity" id="BPMNShape_decidirAquisicaoEnviarNotificacaoProponenteAceiteCallActivity">
