      <extensionElements>
        <activiti:field name="serviceURL">
          <activiti:expression>${polos.get(loopCounter).get("apiUrl").asText()}</activiti:expression>
        </activiti:field>
        <activiti:field name="method">
          <activiti:string>GET</activiti:string>
        </activiti:field>
        <activiti:field name="outputVariable">
          <activiti:expression>${polos.get(loopCounter).get("name").asText()}</activiti:expression>
        </activiti:field>
        <activiti:field name="headers">
          <activiti:string>{"Content-Type": "application/json"}</activiti:string>
        </activiti:field>
      </extensionElements>
      <multiInstanceLoopCharacteristics isSequential="false">
        <loopCardinality>${polos.size()}</loopCardinality>
      </multiInstanceLoopCharacteristics>
    </serviceTask>
    <sequenceFlow id="flow3" sourceRef="daParecerBuscaPolosRestTask" targetRef="daParecerBuscaExemplaresPorPoloRestTask"></sequenceFlow>
