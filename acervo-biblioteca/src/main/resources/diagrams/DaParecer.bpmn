      <extensionElements>
        <activiti:field name="serviceURL">
          <activiti:string>http://www.mocky.io/v2/5e6cdc3b2e000072000eea6e</activiti:string>
        </activiti:field>
        <activiti:field name="method">
          <activiti:string>GET</activiti:string>
        </activiti:field>
        <activiti:field name="outputVariable">
          <activiti:string>polos</activiti:string>
        </activiti:field>
        <activiti:field name="headers">
          <activiti:string>{"Content-Type": "application/json"}</activiti:string>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <parallelGateway id="daParecerObterInformacaoParallelGatewayStart" name="Obter Informação Start"></parallelGateway>
    <sequenceFlow id="flow2" sourceRef="daParecerObterInformacaoParallelGatewayStart" targetRef="daParecerBuscaPolosRestTask"></sequenceFlow>
