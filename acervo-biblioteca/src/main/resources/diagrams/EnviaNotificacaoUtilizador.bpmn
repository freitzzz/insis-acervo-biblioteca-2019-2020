        <omgdi:waypoint x="546.0" y="100.0"></omgdi:waypoint>
        <omgdi:waypoint x="547.0" y="146.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</definitions>