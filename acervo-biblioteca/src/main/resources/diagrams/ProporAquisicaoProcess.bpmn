    <sequenceFlow id="flow14" sourceRef="proporAquisicaoValidaInformacaoPropostaScriptTask" targetRef="proporAquisicaoValidaInformacaoPropostaExclusiveGateway"></sequenceFlow>
    <sequenceFlow id="proporAquisicaoInformacaoValidada" name="Informação Validada" sourceRef="proporAquisicaoValidaInformacaoPropostaExclusiveGateway" targetRef="proporAquisicaoBuscarInformacaoEndEvent">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${informacaoValida == 'true'}]]></conditionExpression>
    </sequenceFlow>
