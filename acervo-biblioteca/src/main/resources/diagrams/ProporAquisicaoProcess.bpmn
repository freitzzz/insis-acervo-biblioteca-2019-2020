    <sequenceFlow id="flow11" sourceRef="proporAquisicaoBuscarInformacaoAgrupaInformacaoScriptTask" targetRef="proporAquisicaoFlowNormalUserTask"></sequenceFlow>
    <scriptTask id="proporAquisicaoValidaInformacaoPropostaScriptTask" name="Valida Informação Proposta" scriptFormat="groovy" activiti:autoStoreVariables="false">
      <script>// valida peritos

def peritosSplit = peritos.split(";")

def areDefinedPeritosValid = peritosSplit.length &gt;= 2

def processInstanceId = execution.getProcessInstanceId()

println(processInstanceId)

execution.setVariableLocal('processInstanceId', processInstanceId)

def errorMessage = ""

if(!areDefinedPeritosValid) {

	errorMessage = "Informação dos peritos inválida. Por favor insira o nome de pelo menos dois peritos separados por ;"

}

execution.setVariableLocal('informacaoValida', areDefinedPeritosValid)

execution.setVariableLocal('errorHeader', errorMessage)</script>
    </scriptTask>
    <sequenceFlow id="flow12" sourceRef="proporAquisicaoFlowNormalUserTask" targetRef="proporAquisicaoValidaInformacaoPropostaScriptTask"></sequenceFlow>
